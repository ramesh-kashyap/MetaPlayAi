function getColor(join) {
    let color = '';
    if (join == 'l') color = 'big';
    else if (join == 'n') color = 'small';
    else if (join == 't') color = 'violet';
    else if (join == 'd') color = 'red';
    else if (join == 'x') color = 'green';
    else if (join == '0') color = 'red-violet';
    else if (join == '5') color = 'green-violet';
    else if (join % 2 == 0) color = 'red';
    else if (join % 2 != 0) color = 'green';
    return color;
}

function fetchAIBonus(page = 1) {
    $.ajax({
        type: "GET",
        url: `/api/webapi/aiBonus?page=${page}&limit=5`,
        dataType: "json",
        success: function (response) {
            if (response.status) {
                MemRender(response.aiBonus, response.currentPage, response.totalPages);
            } else {
                alert('Failed to load AI Bonus data');
                $('#loading-spinner').hide(); // Hide loading spinner even if data load fails
            }
        },
        error: function () {
            alert('Error fetching AI Bonus data');
        }
    });
}

function fetchAIBalance() {
    $.ajax({
        type: "GET",
        url: "/api/webapi/getAIBalance",
        dataType: "json",
        success: function (response) {
            if (response.status) {
                $('.num-total').text(response.ai_balance);
            } else {
                alert('Failed to load AI Balance');
            }
        },
        error: function () {
            alert('Error fetching AI Balance');
        }
    });
}

