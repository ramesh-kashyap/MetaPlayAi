            <script>
               function alertMess(text, sic) {
                    $('body').append(
                        `<div data-v-1dcba851="" class="msg">
                            <div data-v-1dcba851="" class="msg-content v-enter-active v-enter-to" style="">${text}</div>
                        </div>`
                    );
                    setTimeout(() => {
                        $('.msg .msg-content').removeClass('v-enter-active v-enter-to');
                        $('.msg .msg-content').addClass('v-leave-active v-leave-to');
                        setTimeout(() => {
                            $('.msg').remove();
                        }, 100);
                        sic.removeClass('block-click');
                    }, 1000);
                }
    
                $('#attendance-button').click(function(e) {
                    e.preventDefault();
                    $.ajax({
                        type: 'POST',
                        url: '/api/webapi/attendanceBonus',
                        success: function(response) {
                            alertMess(response.message, $('#attendance-button'));
                            if (response.status) {
                                updateAttendanceInfo();
                            }
                        },
                        error: function(xhr, status, error) {
                            alertMess('Failed to process attendance bonus', $('#attendance-button'));
                        }
                    });
                });
    
                function updateAttendanceInfo() {
                    $.ajax({
                        type: 'GET',
                        url: '/api/webapi/getAttendanceInfo',
                        success: function(response) {
                            if (response.status) {
                                $('#attendanceDays').text(response.attendanceDays);
                                $('#accumulatedBonus').text(response.accumulatedBonus.toFixed(2));
                            } else {
                                alertMess('Failed to fetch attendance info', $('body'));
                            }
                        },
                        error: function(xhr, status, error) {
                            alertMess('Failed to fetch attendance info', $('body'));
                        }
                    });
                }
    
                $(document).ready(function() {
                    updateAttendanceInfo();
                });
            </script>
